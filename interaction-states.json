          "border": "none",
          "borderRadius": "0.375rem",
          "padding": "0.5rem 1rem",
          "height": "2.25rem",
          "fontSize": "0.875rem",
          "fontWeight": 500,
          "boxShadow": "none",
          "cursor": "pointer",
          "outline": "none"
        },
        "hover": {
          "backgroundColor": "#61534a",
          "backgroundColorCalculation": "primary/90 (90% opacity)",
          "textColor": "#faf8f4",
          "transform": "none",
          "boxShadow": "none",
          "cursor": "pointer",
          "transition": "all 200ms ease"
        },
        "focus": {
          "backgroundColor": "#6b5d52",
          "borderColor": "#b8a68f",
          "borderColorVar": "var(--ring)",
          "ring": "3px solid rgba(184, 166, 143, 0.5)",
          "ringVar": "ring-ring/50",
          "ringWidth": "3px",
          "ringColor": "rgba(184, 166, 143, 0.5)",
          "outline": "none"
        },
        "active": {
