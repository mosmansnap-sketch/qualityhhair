          "backgroundColor": "#d4c5b0",
          "backgroundColorVar": "var(--secondary)",
          "textColor": "#3d3027",
          "textColorVar": "var(--secondary-foreground)",
          "border": "1px solid transparent"
        },
        "hover": {
          "backgroundColor": "#bfb09e",
          "backgroundColorCalculation": "secondary/90"
        }
      }
    },

    "outline": {
      "states": {
        "default": {
          "backgroundColor": "transparent",
          "textColor": "#3d3027",
          "textColorVar": "var(--foreground)",
          "border": "1px solid",
          "borderColor": "rgba(107, 93, 82, 0.15)"
        },
        "hover": {
          "backgroundColor": "#b8a68f",
          "backgroundColorVar": "var(--accent)",
          "textColor": "#3d3027",
          "textColorVar": "var(--accent-foreground)"
        }
      }
    },

    "destructive": {
      "states": {
        "default": {
          "backgroundColor": "#c55a4a",
          "backgroundColorVar": "var(--destructive)",
          "textColor": "#ffffff",
          "border": "1px solid transparent"
        },
        "hover": {
          "backgroundColor": "#b15143",
          "backgroundColorCalculation": "destructive/90"
        },
        "focus": {
          "ring": "3px solid rgba(197, 90, 74, 0.2)"
        }
      }
    }
  },

  "tooltips": {
    "states": {
      "hidden": {
        "opacity": 0,
        "visibility": "hidden",
        "transform": "scale(0.95)",
        "pointerEvents": "none"
      },
      "visible": {
        "opacity": 1,
        "visibility": "visible",
        "transform": "scale(1)",
        "pointerEvents": "auto",
        "backgroundColor": "#3d3027",
        "backgroundColorVar": "var(--foreground)",
        "textColor": "#f5f1eb",
        "textColorVar": "var(--background)",
        "padding": "0.5rem 0.75rem",
        "borderRadius": "0.375rem",
        "fontSize": "0.875rem",
        "boxShadow": "0 10px 15px -3px rgba(0, 0, 0, 0.3)",
        "zIndex": 70
      }
    },
    "transitions": {
      "property": "opacity, transform, visibility",
      "duration": "150ms",
      "timingFunction": "ease-in-out"
    }
  },

  "dropdowns": {
    "trigger": {
      "inherits": "buttons.outline",
      "states": {
        "open": {
          "backgroundColor": "#b8a68f",
          "backgroundColorVar": "var(--accent)",
          "borderColor": "#b8a68f"
        }
      }
    },
    "menu": {
      "states": {
        "closed": {
          "opacity": 0,
          "transform": "scale(0.95)",
          "visibility": "hidden"
        },
        "open": {
          "opacity": 1,
          "transform": "scale(1)",
          "visibility": "visible",
          "backgroundColor": "#faf8f4",
          "backgroundColorVar": "var(--popover)",
          "border": "1px solid",
          "borderColor": "rgba(107, 93, 82, 0.15)",
          "borderRadius": "0.375rem",
          "padding": "0.25rem",
          "boxShadow": "0 4px 6px -1px rgba(0, 0, 0, 0.1), 0 2px 4px -2px rgba(0, 0, 0, 0.1)",
          "zIndex": 50
        }
      },
      "transitions": {
        "enter": {
          "duration": "200ms",
          "timingFunction": "ease-out"
        },
        "exit": {
          "duration": "150ms",
          "timingFunction": "ease-in"
        }
      }
    },
    "menuItem": {
      "states": {
        "default": {
          "backgroundColor": "transparent",
          "textColor": "#3d3027",
          "padding": "0.5rem 0.75rem",
          "borderRadius": "0.25rem",
          "cursor": "pointer"
        },
        "hover": {
          "backgroundColor": "#b8a68f",
          "backgroundColorVar": "var(--accent)",
          "textColor": "#3d3027",
          "textColorVar": "var(--accent-foreground)"
        },
        "focus": {
          "backgroundColor": "#b8a68f",
          "outline": "none"
        },
        "active": {
          "backgroundColor": "#a89680"
        },
        "disabled": {
          "opacity": 0.5,
          "cursor": "not-allowed",
          "pointerEvents": "none"
        }
      },
      "transitions": {
        "property": "background-color, color",
        "duration": "150ms",
        "timingFunction": "ease"
      }
    }
  },

  "modals": {
    "overlay": {
      "states": {
        "closed": {
          "opacity": 0,
          "visibility": "hidden"
        },
        "open": {
          "opacity": 1,
          "visibility": "visible",
          "backgroundColor": "rgba(0, 0, 0, 0.5)",
          "backdropFilter": "blur(4px)",
          "zIndex": 40
        }
      },
      "transitions": {
        "duration": "200ms",
        "timingFunction": "ease-in-out"
      }
    },
    "dialog": {
      "states": {
        "closed": {
          "opacity": 0,
          "transform": "scale(0.95)",
          "visibility": "hidden"
        },
        "open": {
          "opacity": 1,
          "transform": "scale(1)",
          "visibility": "visible",
          "backgroundColor": "#f5f1eb",
          "backgroundColorVar": "var(--background)",
          "border": "1px solid",
          "borderColor": "rgba(107, 93, 82, 0.15)",
          "borderRadius": "0.5rem",
          "padding": "1.5rem",
          "boxShadow": "0 20px 25px -5px rgba(0, 0, 0, 0.1), 0 8px 10px -6px rgba(0, 0, 0, 0.1)",
          "zIndex": 50
        }
      },
      "transitions": {
        "duration": "200ms",
        "timingFunction": "ease-out"
      }
    }
  },

  "accordions": {
    "trigger": {
      "states": {
        "closed": {
          "backgroundColor": "transparent",
          "iconRotation": "0deg"
        },
        "open": {
          "backgroundColor": "transparent",
          "iconRotation": "180deg"
        },
        "hover": {
          "backgroundColor": "#e8dfd4",
          "backgroundColorVar": "var(--muted)",
          "cursor": "pointer"
        },
        "focus": {
          "ring": "3px solid rgba(184, 166, 143, 0.5)",
          "outline": "none"
        }
      },
      "transitions": {
        "property": "background-color",
        "duration": "200ms",
        "iconTransition": {
          "property": "transform",
          "duration": "200ms",
          "timingFunction": "ease-in-out"
        }
      }
    },
    "content": {
      "states": {
        "closed": {
          "height": "0",
          "opacity": 0,
          "overflow": "hidden"
        },
        "open": {
          "height": "auto",
          "opacity": 1,
          "overflow": "visible"
        }
      },
      "transitions": {
        "property": "height, opacity",
        "duration": "200ms",
        "timingFunction": "ease-in-out"
      }
    }
  },

  "tabs": {
    "trigger": {
      "states": {
        "inactive": {
          "backgroundColor": "transparent",
          "textColor": "#7a6d5f",
          "textColorVar": "var(--muted-foreground)",
          "borderBottom": "2px solid transparent",
          "cursor": "pointer"
        },
        "active": {
          "backgroundColor": "transparent",
          "textColor": "#3d3027",
          "textColorVar": "var(--foreground)",
          "borderBottom": "2px solid",
          "borderBottomColor": "#6b5d52",
          "borderBottomColorVar": "var(--primary)",
          "fontWeight": 500
        },
        "hover": {
          "backgroundColor": "#e8dfd4",
          "backgroundColorVar": "var(--muted)",
          "textColor": "#3d3027"
        },
        "focus": {
          "ring": "3px solid rgba(184, 166, 143, 0.5)",
          "outline": "none"
        },
        "disabled": {
          "opacity": 0.5,
          "cursor": "not-allowed"
        }
      },
      "transitions": {
        "property": "all",
        "duration": "200ms",
        "timingFunction": "ease"
      }
    },
    "content": {
      "states": {
        "hidden": {
          "display": "none",
          "opacity": 0
        },
        "visible": {
          "display": "block",
          "opacity": 1
        }
      },
      "transitions": {
        "property": "opacity",
        "duration": "200ms"
      }
    }
  },

  "globalInteractionPatterns": {
    "focusRing": {
      "width": "3px",
      "color": "rgba(184, 166, 143, 0.5)",
      "colorVar": "ring-ring/50",
      "offset": "0px",
      "style": "solid"
    },
    "transitionDefaults": {
      "duration": "200ms",
      "timingFunction": "ease",
      "property": "all"
    },
    "disabledState": {
      "opacity": 0.5,
      "cursor": "not-allowed",
      "pointerEvents": "none"
    },
    "loadingState": {
      "cursor": "wait",
      "opacity": 0.7,
      "pointerEvents": "none"
    },
    "motionPreferences": {
      "description": "Respects user's prefers-reduced-motion",
      "reducedMotion": {
        "transitionDuration": "0ms",
        "animationDuration": "0ms"
      }
    }
  },

  "customAnimations": {
    "cardHoverLift": {
      "transform": "translateY(-8px) scale(1.02)",
      "boxShadow": "0 20px 40px rgba(0, 0, 0, 0.15)",
      "duration": "300ms",
      "timingFunction": "cubic-bezier(0.4, 0, 0.2, 1)"
    },
    "buttonPulse": {
      "hover": {
        "scale": 1.05,
        "transition": "spring(stiffness: 400, damping: 10)"
      },
      "tap": {
        "scale": 0.95,
        "transition": "spring(stiffness: 400, damping: 10)"
      }
    },
    "fadeIn": {
      "from": {
        "opacity": 0
      },
      "to": {
        "opacity": 1
      },
      "duration": "200ms"
    },
    "slideUp": {
      "from": {
        "transform": "translateY(20px)",
        "opacity": 0
      },
      "to": {
        "transform": "translateY(0)",
        "opacity": 1
      },
      "duration": "300ms"
    },
    "scaleIn": {
      "from": {
        "transform": "scale(0.95)",
        "opacity": 0
      },
      "to": {
        "transform": "scale(1)",
        "opacity": 1
      },
      "duration": "200ms"
    }
  },

  "accessibilityStates": {
    "ariaInvalid": {
      "input": {
        "borderColor": "#c55a4a",
        "borderColorVar": "var(--destructive)",
        "ring": "3px solid rgba(197, 90, 74, 0.2)"
      },
      "description": "Applied when aria-invalid='true'"
    },
    "ariaDisabled": {
      "opacity": 0.5,
      "cursor": "not-allowed",
      "pointerEvents": "none"
    },
    "ariaSelected": {
      "backgroundColor": "#6b5d52",
      "backgroundColorVar": "var(--primary)",
      "textColor": "#faf8f4",
      "textColorVar": "var(--primary-foreground)"
    }
  }
}
